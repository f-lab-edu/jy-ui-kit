{
  "compilerOptions": {
    "target": "ES2016",
    "lib": ["DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "module": "CommonJS",
    "declaration": true,
    "declarationMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
